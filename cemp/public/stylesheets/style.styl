body
  padding: 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
  font-size 13px
  text-align center
  text-stroke 1px rgba(255, 255, 255, 0.1)
  color #555
  background #FFFFFF
a
  color: #212121
h1, h2
  margin 0
  font-size 22px
  color #343434
h1
  text-shadow 1px 2px 2px #ddd
  font-size 60px
.h8
    font-size 10px
    color #343434
    padding 0px
  
#employees
  text-align left
  margin-left auto
  margin-right auto
  width 320px
  .employee
    margin 20px
    .created_at
        display none
    .title
        font-weight bold
        background-color #eee
    .name
        background-color #ffa
