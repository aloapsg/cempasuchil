.dropbtn
    background-color: #FFFFFF
    padding: 5px
    font-size: 14px
    border 1px
    cursor pointer
    border-radius 10px
    border-color #ee9528
    border-style solid
    margin-left 5%
    margin-right auto
    width 150px
.dropbtn a
    text-decoration: none
.dropdown
    display: inline-block
    margin-left 1%
    margin-right auto
    font-size: 14px

.dropdown-content
    display: none
    position: absolute
    background-color: #f9f9f9
    min-width: 160px
    box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2)
    z-index: 1

.dropdown-content a
    color: black
    padding: 12px 16px
    text-decoration: none
    display: block

.dropdown-content a:hover 
    background-color: #f1f1f1

.dropdown:hover .dropdown-content
    display: block
